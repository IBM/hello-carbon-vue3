{
  "name": "hello-carbon-nuxt",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "PORT=4507 nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint . --fix",
    "dev:e2e": "npx cypress open --e2e",
    "test:e2e": "npx cypress run --e2e --browser chrome",
    "test:e2e:ff": "npx cypress run --e2e --browser firefox",
    "test:e2e:edge": "npx cypress run --e2e --browser edge",
    "test:e2e:safari": "npx cypress run --e2e --browser safari",
    "prepare": "husky"
  },
  "dependencies": {
    "@carbon/icons-vue": "^10.106.0",
    "@carbon/themes": "^10.55.3",
    "@carbon/vue": "^3.0.24",
    "@nuxt/eslint": "^1.1.0",
    "@nuxtjs/i18n": "^9.4.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@sidebase/nuxt-auth": "^0.10.0",
    "luxon": "^3.5.0",
    "next-auth": "~4.21.1",
    "nuxt": "^3.16.2",
    "vue": "latest",
    "vue-router": "latest",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cypress": "^14.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.4.3",
    "sass": "^1.85.1"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint --fix"
  }
}
